<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="9dip" />
            <stroke android:width="1dip" android:color="#E91E63" />
            <gradient android:angle="-90" android:startColor="#C51A54" android:endColor="#EC608F"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="9dip" />
            <stroke android:width="1dip" android:color="#E91E63" />
            <solid android:color="#C51A54"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="9dip" />
            <stroke android:width="1dip" android:color="#E91E63" />
            <gradient android:angle="-90" android:startColor="#EC608F" android:endColor="#C51A54" />
        </shape>
    </item>
</selector>